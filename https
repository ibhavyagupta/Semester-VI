                                  src="https://www.natwest.com/etc.clientlibs/responsive/components/foundation/rte/clientlibs/clientlib_rte_base-_a3c181f8d86ade1f1b0a1b6f1d5b0e57.min.js"></script>
                                <script>
                                  const enableAnimation = !globalUtil.isReducedMotion();
                                  // when mouse pointer enters the card
                                  function onCardMouseEnter(index) {
                                    if (enableAnimation) {
                                      if (index == 0 || index == 1 || index == 3) { // For Dynamic Card 1 and 2
                                        document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[0].style.transform = 'scale(1.3)';
                                      }
                                      if (index == 2) { // For Dynamic Card 3
                                        if (window.outerWidth < 768) {
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.top = '23%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.transform = 'rotate(0.5deg)';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[3].style.top = '61%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[4].style.top = '80%';
                                        } else {
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.top = '23%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.transform = 'rotate(0.5deg)';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.top = '42%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[3].style.top = '61%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[4].style.top = '80%';

                                        }
                                      }
                                      if (index == 3) { // For Dynamic Card 4
                                        document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[0].style.transform = 'scale(1.18)';
                                      }
                                      if (index == 4) { // For Dynamic Card 5
                                        document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[0].style.transform = 'scale(1.2)';
                                        if (window.outerWidth > 1023 || window.outerWidth < 1255) { // L Screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '30px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '30px';
                                        }
                                        if (window.outerWidth > 767 || window.outerWidth < 1024) { // M Screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '23px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '23px';
                                        }
                                        if (window.outerWidth > 1254) { // XL Screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '12px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '12px';
                                        }
                                        if (window.outerWidth < 479) { // S screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '105px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '105px';
                                        } else if (window.outerWidth < 768) { // S screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '80px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '80px';
                                        }
                                      }
                                    }
                                  }

                                  // when mouse pointer leaves the card
                                  function onCardMouseLeave(index) { // For All Dynamic Cards
                                    if (enableAnimation) {
                                      document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[0].style.transform = 'scale(1)';
                                      if (index == 2) { // For Dynamic Card 3
                                        if (window.outerWidth < 768) {
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.top = '25%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.transform = 'rotate(0deg)';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.top = '42%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.transform = 'rotate(0deg)';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[3].style.top = '57.9%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[4].style.top = '74%';
                                        } else {
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.top = '25%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.transform = 'rotate(0deg)';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.top = '41%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.transform = 'rotate(0deg)';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[3].style.top = '56%';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[4].style.top = '71%';

                                        }
                                      }
                                      if (index == 4) { // For Dynamic Card 5
                                        if (window.outerWidth > 1023 || window.outerWidth < 1255) { // L Screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '40px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '40px';
                                        }
                                        if (window.outerWidth > 767 || window.outerWidth < 1024) { // M Screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '30px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '30px';
                                        }
                                        if (window.outerWidth > 1254) { // XL screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '23px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '23px';
                                        }
                                        if (window.outerWidth < 479) { // S screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '118px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '118px';
                                        } else if (window.outerWidth < 768) { // S screen
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[1].style.bottom = '90px';
                                          document.querySelectorAll('.wr-card-content')[index].getElementsByTagName('img')[2].style.bottom = '90px';
                                        }
                                      }
                                    }
                                  }

                                  // Reloading clientlibs require for tooltip to render properly
                                  const clientLibList = [];
                                  function removeDuplicateSrc(acc, curr) {
                                    if (!acc.includes(curr)) acc.push(curr);
                                    return acc;
                                  }
                                  window.addEventListener('load', () => {
  console.log('window load completes!');
  const tooltipClientLibName = 'clientlib_rte_base';
  // find the existing clientlib require for tooltip and remove
  const allClientLibs = document.querySelectorAll('script');
  allClientLibs.forEach(clientlib => {
    if (clientlib.src && clientlib.src.includes(tooltipClientLibName)) {
      clientLibList.push(clientlib.src);
      clientlib.parentNode.removeChild(clientlib);
    }
  });
  // remove duplicate clientlib src
  const newClientLibSrcList = clientLibList.reduce(removeDuplicateSrc, []);

  // create and append the clientlibs require for tooltip
  newClientLibSrcList.forEach(newClientLibSrc => {
    const newClientLib = document.createElement('script');
    newClientLib.src = newClientLibSrc;
    newClientLib.defer = 'true';
    document.head.appendChild(newClientLib);
  });

  // ✅ ADD THIS: Ensure popovers work on touch devices
  setTimeout(() => { // give time for tooltip scripts to load
    const popElements = document.querySelectorAll('[data-toggle="popover"]');

    popElements.forEach(function (el) {
      $(el).popover({
        trigger: 'click', // tap or click instead of hover
        html: true
      });

      // Close popover when clicking outside
      document.addEventListener('click', function (e) {
        if (!el.contains(e.target)) {
          $(el).popover('hide');
        }
      });
    });
  }, 500); // small delay to ensure clientlibs are loaded
});


                                </script>
                              </section>
